<tr>
    <th class="text-center" scope="row"><img class="img-avatar img-avatar32" src="<%= @room.logo %>" alt="">
    </th>
    <td><%= @room.name %></td>
    <td class="text-center">
        <div class="btn-group">
            <%= link to: room_path(@conn, :show, @room) do %>
            <button type="button" class="btn btn-sm btn-secondary" data-toggle="tooltip" title="Join">
                <i class="fa fa-pencil"></i>
            </button>
            <% end %>
        </div>
    </td>
</tr>